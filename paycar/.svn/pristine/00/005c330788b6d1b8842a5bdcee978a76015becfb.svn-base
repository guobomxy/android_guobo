<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@android:id/tabhost"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#333333" >

        <RelativeLayout
        android:id="@+id/rl2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="8dp"
        android:paddingBottom="4dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@color/titlebar" >

        <ImageView
            android:id="@+id/tip1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="0dp"
            android:src="@drawable/icon_5_d" />

        <TextView
            android:id="@+id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@+id/tip1"
            android:text="@string/jiaoche"
            android:textColor="@color/titletext"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/text1"
            android:layout_toLeftOf="@+id/beizhutip"
            android:src="@drawable/jiantou2" />

        <ImageView
            android:id="@+id/saomatip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/text1"
            android:layout_toLeftOf="@+id/imageView2"
            android:src="@drawable/saoma2" />

        <ImageView
            android:id="@+id/paizhaotip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView3"
            android:layout_toLeftOf="@+id/imageView3"
            android:src="@drawable/paizhao3" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/paizhaotip"
            android:layout_toLeftOf="@+id/paizhaotip"
            android:src="@drawable/jiantou2" />

        <ImageView
            android:id="@+id/beizhutip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/text1"
            android:src="@drawable/beizhu3" />

    </RelativeLayout>
    
	<include 
	    android:id="@+id/include1"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/rl2"
	    layout="@layout/paycar_saoma" />
	<include 
	   	android:id="@+id/include2"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/rl2"
	    layout="@layout/paycar_paizhao"/>
    <include 
        android:id="@+id/include3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl2"
        layout="@layout/paycar_beizhu"/>
    
  </RelativeLayout>  

