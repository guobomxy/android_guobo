<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eastime.paycar"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ca11"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.eastime.paycar.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.eastime.paycar.activitys.Manage"
            android:icon="@drawable/d"
            android:label="@string/title_activity_manage1" >
        </activity>
        <activity android:name="com.eastime.paycar.activitys.AActivity" />
        <activity android:name="com.eastime.paycar.activitys.BActivity" />
        <activity android:name="com.eastime.paycar.activitys.CActivity" />
        <activity android:name="com.eastime.paycar.activitys.EActivity" />
        <activity
            android:name="com.eastime.paycar.activitys.Photo"
            android:label="@string/title_activity_photo" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" >
    </uses-permission>

</manifest>